line2 = line[:]
result = 0
i = 0
value = 1
while len(line) > 13:
    arr = list(line[i-1:i + 12])
    for x in arr:
        value *= int(x)
    if result < value:
        result = value
    i += 1
    line = line2[i:]
    value = 1
print(result)
